- content_for :javascript do
  initialize_browse_view('#{@title}','#{I18n.locale}');

#search_page.grid_24
  .grid_6.alpha
    %h1= t(".#{@title}")
    #left_link_panel_browse
      .left_panel_section
        .left_panel_title
          %h3= t(".category")
        = link_to t(".all_categories"), "#", :class => "selected", :name => "categories_all"
        = render :partial => "left_panel_link", :collection => Listing::VALID_CATEGORIES, :as => :title, :locals => { :type => "categories" }

  .grid_18.omega
    #search_form
      = form_tag({ :action => :show, :controller => :search }, :method => "get") do
        = text_field_tag("q", params[:q], :class => "text_field")
        = submit_tag(t(".search_verb"), :class => "send_button")
    #browse_listings_tabs
      %div{:class => get_listing_tab_class("list_view")} 
        = link_to t('.list_view'), "#"
      %div{:class => get_listing_tab_class("map_view")} 
        = link_to t('.map_view'), "#"
    #profile_tab_spacer
    #search_results
      = render :partial => "listed_listings"
      = pageless(@listings.total_pages, '#search_results', browse_path(:listing_type => @title, :category => params[:category]), t(".loading_more_#{@title}"))